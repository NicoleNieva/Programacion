#include "stdafx.h"
#include "conio.h"
#include "stdlib.h"
#include "stdio.h"


void main()
{
int A,B;
printf ("\n\n Ingrese un numero: ");
scanf ("%d",&A);
printf ("\n\n Ingrese otro numero: ");
scanf ("%d",&B);
if (A > B)
   {printf("\n\n %d es Mayor que  %d ",A,B);
   }
else 
   {if (A == B)
	  {printf("\n\n %d es Igual que  %d ",A,B);
	  }
    else
      {printf("\n\n %d es Menor que  %d ",A,B);
	  }
  }
printf("\n\n");
system("pause");

}
